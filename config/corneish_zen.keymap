    //zmk,matrix_transform = &five_column_transform;
  };
  
  
  macros {
        swleft: SwitchLeft {
            label = "SwitchLeft";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&kp LG(LA(LC(LS(LEFT))))>
                , <&to mCHAR>
                ;
        };
    };

  	macros {
        swright: SwitchRight {
            label = "SwitchRight";
